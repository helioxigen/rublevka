{
  "version": 2,
  "build": {
    "env": {
      "REACT_APP_FIREBASE_API_KEY": "@rublevka-export-app-react-app-firebase-api-key",
      "REACT_APP_FIREBASE_AUTH_DOMAIN": "@rublevka-export-app-react-app-firebase-auth-domain",
      "REACT_APP_FIREBASE_PROJECT_ID": "@rublevka-export-app-react-app-firebase-project-id"
    }
  },
  "builds": [
    {
      "src": "export-frontend/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "export-frontend/build"
      }
    }
  ]
}
